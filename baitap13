import math
x1 = float(int(input("Nhập tọa độ x1 của điểm A: ")))
y1 = float(int(input("Nhập tọa độ y1 của điểm A: ")))
x2 = float(int(input("Nhập tọa độ x2 của điểm B: ")))
y2 = float(int(input("Nhập tọa độ y2 của điểm B: ")))   

d = math.sqrt((x2 - x1)**2 + (y2 - y1)**2)
print("Khoảng cách giữa hai điểm là:", d)
